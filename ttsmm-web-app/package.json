	"prettier": {
		"overrides": [
			{
				"files": [
					".prettierrc",
					".eslintrc"
				],
				"options": {
					"parser": "json",
					"trailingComma": "none",
					"useTabs": true
				}
			}
		],
		"singleQuote": true,
		"trailingComma": "none",
		"tabWidth": 2,
		"useTabs": true,
		"printWidth": 140,
		"bracketSameLine": false
	}
