{
	"name": "ttsmm-web-app",
	"homepage": "https://flsoz.github.io/ttsmm-web",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@ant-design/colors": "^6.0.0",
		"@ant-design/icons": "^4.7.0",
		"@craco/craco": "^6.0.0",
		"@emotion/react": "^11.4.1",
		"@emotion/styled": "^11.3.0",
		"@fontsource/roboto": "^4.5.1",
